{
    "name": "pderas/azure-redis",
    "version": "0.2.1",
    "description": "Handles Azure Redis connection using Azure Managed Identities",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pderas\\AzureRedisAuth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Brock Roadhouse",
            "email": "brock.roadhouse@fabledsolutions.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/redis": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/encryption": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pderas\\AzureRedisAuth\\Providers\\AzureRedisAuthServiceProvider"
            ]
        }
    }
}
